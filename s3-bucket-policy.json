{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowTranscribeAccess",
      "Effect": "Allow",
      "Principal": {
        "Service": "transcribe.amazonaws.com"
      },
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::toori360/*"
      ]
    },
    {
      "Sid": "AllowLambdaAccess",
      "Effect": "Allow",
      "Principal": {
        "Service": "lambda.amazonaws.com"
      },
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::toori360/*",
        "arn:aws:s3:::toori-chat-history/*"
      ]
    }
  ]
}